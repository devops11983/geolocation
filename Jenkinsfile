pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build step'
                sleep 10
            }
        }
                stage('Test') {
            steps {
                echo 'Test steep'
            }
        }
                stage('Deploy') {
            steps {
                echo 'Deploy steep'
                sleep 10
            }
        }
                stage('Docker') {
            steps {
                echo 'Image 'steep'
            }
        }
    }
}
